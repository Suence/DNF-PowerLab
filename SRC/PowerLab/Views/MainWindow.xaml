<Window x:Class="PowerLab.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:constants="clr-namespace:PowerLab.Core.Constants;assembly=PowerLab.Core"
        xmlns:prism="http://prismlibrary.com/"
        Title="{Binding Source={StaticResource R}, Path=Labels.AppName}"
        Width="525"
        Height="350"
        prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static constants:RegionNames.MainRegion}" />
    </Grid>
</Window>
